{
  "name": "fond-of-spryker/oms-octopus-connector",
  "description": "Oms octopus connector module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Felix Pfister",
      "email": "felix@pfister.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0"
  },
  "require-dev": {
    "spryker/oms": "*",
    "spryker/sales": "*",
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "^2.4",
    "mikey179/vfsStream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "suggest": {
    "spryker/oms": "If you want to use Oms command plugins.",
    "spryker/sales": "If you want to use ReleaseUsedVoucherCodes plugin."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    }
  }
}
